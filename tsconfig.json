{
    "compileOnSave": true,
    "compilerOptions": {
        "outDir": "./built",
        "allowJs": true,
        "diagnostics": true,
        "checkJs": false,
        "target": "es5",
        "baseUrl": "./",
        "types": [
            "cypress"
        ],
        "paths": {
            "@/*": [
                "cypress/*"
            ]
        }
    },
    "include": [
        "**/*.js"
    ],
    "exclude": [
        "node_modules",
        "./built/**/*"
    ]
}